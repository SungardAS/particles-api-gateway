---
statusCode: "",
contentType: "application/json"
allowedMethods: ""
allowedHeaders: ""
allowedOrigin: "'*'"
---
  {
    "StatusCode": {{helper "m:core" "propertyValue" statusCode}},
    "ResponseModels": {
      {{helper "m:core" "propertyValue" contentType}}: "Empty"
    },
    "ResponseParameters": {
      {{#if allowedMethods}}
        "method.response.header.Access-Control-Allow-Methods": {{helper "m:core" "propertyValue" allowedMethods}},
      {{/if}}
      {{#if allowedHeaders}}
        "method.response.header.Access-Control-Allow-Headers": {{helper "m:core" "propertyValue" allowedHeaders}},
      {{/if}}
      "method.response.header.Access-Control-Allow-Origin": {{helper "m:core" "propertyValue" allowedOrigin}}
    }
